        {
          "match": "^ +[0-9]+",
          "style": "annotation",
          "name": "line.number"
        },
        {
          "match": "\\* .*",
          "style": "single-line-comment",
          "name": "comment.singleLine"
        },
        {
          "match": "\"(?:\\\\\"|[^\"])+?\"",
          "style": "string",
          "name": "string"
        },
        {
          "match": "<[-+]?[0-9.]+([xX][a-fA-F0-9]+|b[01]+|e[-+0-9]+)?>",
          "style": "literal",
          "name": "literal.numeric"
        },
        {
          "match": "[-+&|<>=~?]{1,2}",
          "style": "operator",
          "name": "keyword.operator"
        }
