{
  "grammars": [
    {
      "scopeName": "json",
      "patterns": [
        {
          "match": "<\"[a-z0-9_-]\">",
          "name": "string.json.key",
          "style": "string"
        },
        {
          "match": "<(true|false|null)>",
          "name": "keyword.control.json",
          "style": "keyword"
        },
        {
          "match": "[\\[\\]{}]",
          "name": "keyword.operator.json",
          "style": "identifier"
        },
        {
          "match": "<[0-9.]+>",
          "name": "constant.numeric",
          "style":  "literal"
        }
      ]
    }
  ]
}
