{
  "grammars": [
    {
      "scopeName": "json",
      "patterns": [
        {
          "match": "<\"[A-Za-z0-9_-]+\":>",
          "name": "string.json.key",
          "style": "string"
        },
        {
          "match": "<\"[^\"]+\">",
          "name": "string.json.value",
          "style": "literal"
        },
        {
          "match": "<(true|false|null)>",
          "name": "keyword.control.json",
          "style": "keyword"
        },
        {
          "match": "[\\[\\]{}]",
          "name": "keyword.operator.json",
          "style": "operator"
        },
        {
          "match": "<[0-9.]+>",
          "name": "constant.numeric",
          "style": "literal"
        }
      ]
    }
  ]
}
