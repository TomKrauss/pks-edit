{
  "grammars": [
    {
      "scopeName": "java",
      "description": "Java Programming Language",
      "import": [ "commonc" ],
      "tagSources": [
        {
          "filename": "tags",
          "type": "tags"
        },
        {
          "filename": "https://www.google.com/search?q=javadoc+$1",
          "type": "url"
        }
      ],
      "templates": [
        {
          "match": "copyright",
          "contents": "${copyright}"
        },
        {
          "name": "file",
          "match": "file",
          "contents": "${copyright}\nclass ${file_name_no_suffix} {\n\n}\n"
        }
      ],
      "patterns": [
        {
          "begin": "/**",
          "end": "*/",
          "style": "comment",
          "patterns": [
            {
              "match": "@[a-zA-Z]+",
              "style": "bold-comment",
              "name": "javadoc.tags"
            }
          ],
          "name": "comment.multiLine"
        },
        {
          "match": "<[A-Z_][A-Z_0-9]*>",
          "style": "identifier",
          "name": "constant.identifier"
        },
        {
          "match": "<[A-Z_][a-zA-Z_0-9]+>",
          "style": "constant",
          "name": "constant.className"
        },
        {
          "keywords": [
            "true",
            "false",
            "if",
            "else",
            "this",
            "super",
            "null",
            "var",
            "return",
            "sealed",
            "record",
            "import",
            "extends",
            "class",
            "interface",
            "package",
            "enum",
            "strictfp",
            "transient",
            "volatile",
            "private",
            "public",
            "protected",
            "synchronized",
            "new",
            "throw",
            "throws",
            "catch",
            "for",
            "while",
            "do",
            "continue",
            "break",
            "static",
            "final",
            "native",
            "const",
            "var",
            "switch",
            "default",
            "float",
            "long",
            "double",
            "int",
            "short",
            "void",
            "byte",
            "try",
            "finally"
          ],
          "style": "keyword",
          "name": "keyword.java"
        },
        {
          "match": "\\<[A-Za-z][A-Za-z0-9]*\\>",
          "style": "italic",
          "name": "generics.identifier"
        },
        {
          "match": "<@[A-Za-z][A-Za-z0-9]*>",
          "style": "annotation",
          "name": "annotation.identifier"
        }
      ]
    }
  ]
}
